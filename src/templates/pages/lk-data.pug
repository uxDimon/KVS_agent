extends ../template

block variables
  - var title = 'ЛК Личные данные'

block content
    main.components-container.lkdata
        .stream-wide
            section.header-with-caption
                h1 Личный кабинет
                p.subtitle В этом разделе отображается вся информация о вашем агентстве.
                .submenu
                    ul
                        li
                            a.current(href="lk-data.html") Личные данные
                        li
                            a(href="lk-staff.html") Сотрудники
                        li
                            a(href="lk-notices.html") Уведомления
                        li
                            a(href="lk-events.html") Календарь мероприятий
        .stream-wide
            form.form.ajax-form(enctype="multipart/form-data" method="POST" action autocomplete="on" novalidate="novalidate")
                ul.fields
                    li.not-empty
                        input.form-control.not-empty(disabled data-required="yes" type="text" id="nameOrg" value="ООО «Название агентства»" name="nameOrg" required="")
                        label(for="nameOrg") Полное название организации
                    li.not-empty
                        input.form-control.not-empty(disabled data-required="yes" type="text" id="kpp" value="111222333555666" name="kpp" required="")
                        label(for="kpp") КПП
                    li.not-empty
                        input.form-control.not-empty(disabled data-required="yes" type="text" id="urAddress" value="194100, г. Санкт-Петербург, ст. м. Лесная, ул. Грибалёвой, дом 9, корпус 1" name="urAddress" required="")
                        label(for="urAddress") Юридический адрес
                    li.not-empty
                        input.form-control.not-empty(disabled data-required="yes" type="text" id="contactPerson" value="Александр Александрович Александров" name="contactPerson" required="")
                        label(for="contactPerson") Контактное лицо
                    li.not-empty
                        input.form-control.not-empty(disabled data-required="yes" type="text" id="phone" value="+7 (911) 211-1223" data-type="phone" name="phone" required="" data-inputmask-mask="+7 (999) 999-9999" data-mask="+7 (999) 999-9999" data-has-mask="" inputmode="text")
                        label(for="phone") Телефон
                    li.not-empty
                        input.form-control.not-empty(disabled data-required="yes" type="text" id="email" value="example@mail.ru" name="email" required="")
                        label(for="email") E-mail
                    li.not-empty
                        input.form-control.not-empty(disabled data-required="yes" type="text" id="inn" value="111222333555666" name="inn" required="")
                        label(for="inn") ИНН
                    li.not-empty
                        input.form-control.not-empty(disabled data-required="yes" type="text" id="nameBank" value="ООО «ВТБ»" name="nameBank" required="")
                        label(for="nameBank") Наименование банка
                    li.not-empty
                        input.form-control.not-empty(disabled data-required="yes" type="text" id="checkingAccount" value="56748493393888" name="checkingAccount" required="")
                        label(for="checkingAccount") Расчетный счет
                    li.not-empty
                        input.form-control.not-empty(disabled data-required="yes" type="text" id="corrAccount" value="874589247934" name="corrAccount" required="")
                        label(for="corrAccount") Корр. счет
                    li.not-empty
                        input.form-control.not-empty(disabled data-required="yes" type="text" id="bik" value="974293" name="bik" required="")
                        label(for="bik") БИК
                    li
                        button.lkdata__edit Редактировать данные
                        button.btn-orange(type="submit" style="display: none") Сохранить
                    li
                        .lkdata__contract.lkdata-contract
